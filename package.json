{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A webapp for managing recipes",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node server/app.js",
    "test": "NODE_ENV=test && nyc --reporter=html mocha -c --compilers js:babel-register  --timeout 15000 tests/",
    "test_travis": "NODE_ENV=test && nyc _mocha -c --report lcovonly --reporter spec --timeout 15000 tests/",
    "coverage": "nyc npm test && nyc report --reporter=text | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillyWunderdog/More-Recipes.git"
  },
  "author": "Gbenga Ode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WillyWunderdog/More-Recipes/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "tests/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "homepage": "https://github.com/WillyWunderdog/More-Recipes#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^8.0.1",
    "md5": "^2.2.1",
    "nodemailer": "^4.1.0",
    "nodemon": "^1.11.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.13.5",
    "sequelize-cli": "^2.8.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "should": "^12.0.0",
    "supertest": "^3.0.0"
  }
}
