{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A webapp for managing recipes",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server/app.js",
    "sd": "nodemon --watch server --exec babel-node server/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/app.js",
    "heroku-postbuild": "sequelize db:migrate && npm run build:client:prod",
    "build:client:prod": "webpack -p --config webpack.config.production.js",
    "build:client": "npm run clean:client && webpack -p --config=webpack.config.development.js",
    "wp": "npm start && webpack-dev-server --hot --history-api-fallback --open --config webpack.config.dev.babel.js --watch",
    "wb": "webpack-dashboard -- webpack-dev-server --history-api-fallback --inline --progress webpack.config.development.js",
    "watch": "webpack --config webpack.config.dev.babel.js --watch",
    "test": "nyc --reporter=html mocha -c --compilers js:babel-register  --timeout 15000 tests/",
    "test_travis": "nyc _mocha -c --report lcovonly --reporter spec --timeout 15000 tests/",
    "coverage": "nyc npm test --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "clean:client": "rimraf client/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillyWunderdog/More-Recipes.git"
  },
  "author": "Gbenga Ode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WillyWunderdog/More-Recipes/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "tests/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "homepage": "https://github.com/WillyWunderdog/More-Recipes#readme",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.13.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
